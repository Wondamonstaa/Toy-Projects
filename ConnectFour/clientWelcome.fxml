<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clientController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
         <children>
            <Label text="Enter IP address:" />
            <TextField fx:id="ipText" promptText="Default 127.0.0.1" />
            <Label text="Enter Port #:" />
            <TextField fx:id="portText" promptText="Default 5555" />
         </children>
      </HBox>
      <Button fx:id="startButton" mnemonicParsing="false" onAction="#startGame" text="Start Game Client" />
   </children>
</VBox>
